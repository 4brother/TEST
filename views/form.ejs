  <script src="../public/bootStrap/js/bootstrap.min.js"></script>
  <script src="../public/angular-1.7.8/angular.min.js"></script>
  <style>
    @media (min-width: 576px) {
      .color {
        color: red;
      }
    }

    @media (min-width: 768px) {
      .color {
        color: green;
      }
    }


    @media (min-width: 992px) {
      .color {
        color: yellow;
      }
    }


    @media (min-width: 1200px) {
      .color {
        color: black;
      }
    }
  </style>
